										precision = <|1|>
									>
								>
							>
							DV_CODED_TEXT matches {*}
						}
					}
				}
			}
			CLUSTER[at0004] occurrences matches {0..1} matches {    -- Histologic Grade
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0026] occurrences matches {0..1} matches {    -- Not assessable
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
